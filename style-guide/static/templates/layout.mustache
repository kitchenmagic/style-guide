</head>

<body class="docs">
	{{#unless hideHeader}}
	<header>
		<nav class="container">
			<div class="logo-menu">
				<a class="brand" href="./index.html">{{makeParentTitle}}</a>
			</div>
		</nav>
	</header>
	{{/unless}}

	{{{content}}}
	{{^hideFooter}}
	<footer>
		<nav class="container">
			<a class="brand javascriptmvc" href="index.html"></a>
			
		</nav>
	</footer>
	{{/hideFooter}}
	
	<script type="text/javascript">
		var docObject = {{{docObjectString}}};
	</script>
	
	{{#if devBuild}}
		<script type="text/javascript"
				data-main="static"
				src="./node_modules/steal/steal.js">
		</script>
	{{else}}
		<script>
			steal = {
				instantiated: {
					"bundles/static.css!$css" : null
				}
			};
		</script>
		<script type="text/javascript"
				data-main="static"
				data-config="package.json!npm"
				data-bundles-path="./static/bundles"
				src="./static/steal.production.js">
		</script>
	{{/if}}
</body>
